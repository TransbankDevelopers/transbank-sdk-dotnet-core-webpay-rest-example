@{
    ViewBag.Title = "Patpass Comercio";
}

<h2>@ViewBag.Title</h2>
<h4>Inscription.Status</h4>

<h4>Request:</h4>
<pre style="background-color: lightyellow;">
POST: https://www.pagoautomaticocontarjetas.cl//restpatpass/v1/services/status
<code class="json">
        {
            "token": "@ViewBag.Token"
        }
    </code>
</pre>

<h4>Response:</h4>
<pre>
<code class="json">
        {
            "UrlVoucher": "@ViewBag.Action"
            "Status": "@ViewBag.Status"
        }
    </code>
</pre>

<script>
    document.globalForm.innerHTML += '<input type="hidden" name="tokenComercio" id="tokenComercio" value="@ViewBag.Token" />'
</script>

<div>
    <span>

        <a href="javascript: document.globalForm.submit();" class="btn btn-primary">Obtener Voucher</a>
    </span>
    <span>

        @Html.ActionLink("Volver al inicio", "Index", "Home", null, new { @class = "btn btn-primary" })
    </span>
</div>




